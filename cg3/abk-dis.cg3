# enclitic postposition
SET EPOSTP = (/^PP:/r) ;

SET POSTP = PP | EPOSTP ;

# conjunctional interrogative prefixes
LIST ConjIPf = Where FromWhere ToWhere When How Why ;

SET CONJIPF = ConjIPf - Q ;

LIST QUEST = Q QFromWhere QSize QToWhere QWhat QWhere QWho ;

LIST LO = /^LO:/r ;

